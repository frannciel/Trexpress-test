<!-- input-->
<div class="form-floating mb-4 mt-5">
    <input class="form-control" id="<%= field %>" name="<%= field %>" type="text" placeholder="Enter your name..." data-sb-validations="required" />
    <label for="<%= field %>"><%= label %></label>
    <div class="invalid-feedback" data-sb-feedback="<%= field %>:required"><%= msgValidation %></div>
</div>